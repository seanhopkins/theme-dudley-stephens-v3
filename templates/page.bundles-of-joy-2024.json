    "text_and_image_LmYtnY": {
      "type": "text-and-image",
      "settings": {
        "image": "shopify://shop_images/Bundlesofjoy1.jpg",
        "image_mask": "square",
        "image2_mask": "none",
        "subtitle": "",
        "title": "More on The Mission",
        "text": "<p>Bundles of Joy serves mothers (and fathers), babies and children who live at or below the poverty line. The organization works directly with food pantries, homeless and domestic violence shelters, schools, hospitals, and individuals to provide everything from diapers to baby gear, books, toys—and everything in between.</p>",
        "button_label": "",
        "button_link": "",
        "liquid": "",
        "button_style": "primary",
        "align_text": "left",
        "image_width": "50",
        "layout": "right",
        "divider": false,
        "top_padding": true,
        "bottom_padding": true,
        "lazyload_images": true
      }
    },
    "0462ca32-1921-42d0-9b0d-239c90c50a09": {
      "type": "rich-text",
      "blocks": {
        "template--14938178256938__0462ca32-1921-42d0-9b0d-239c90c50a09-text-1": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<p><em>“With real support, moms no longer have to choose between feeding and diapering their children, or turning on the heat. It’s so gratifying to know we have helped Bundles of Joy break this cycle of poverty for families in our communities.”</em></p>"
          }
        },
        "text_idcTcG": {
          "type": "text",
          "settings": {
            "enlarge_text": true,
            "text": "<p>- Lauren Stephens, Dudley Stephens Co-Founder</p>"
